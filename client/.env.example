# EmailJS Configuration for Client-side Email Sending
# Get these values from https://www.emailjs.com

# Service ID (Gmail, Outlook, etc.)
REACT_APP_EMAILJS_SERVICE_ID=your_service_id

# Template ID for OTP emails
REACT_APP_EMAILJS_TEMPLATE_ID=your_template_id

# Public Key from EmailJS dashboard
REACT_APP_EMAILJS_PUBLIC_KEY=your_public_key

# Optional: Custom sender configuration
REACT_APP_FROM_EMAIL=sfclothing.74@gmail.com
REACT_APP_FROM_NAME=Seven Four Clothing
